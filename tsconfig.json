{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "esnext"],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "module": "esnext",
      "moduleResolution": "node",
      "jsx": "react",
      "declaration": true,
      "declarationMap": true,
      "esModuleInterop": true
    },
    "include": ["src/**/*.ts", "src/**/*.tsx"],
    "exclude": ["node_modules"]
  }
  